# Vibe-Cast Configuration
# Copy this file to .env and fill in your values

# Anthropic API Key for lyrics translation
# Get your key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_api_key_here

# Output directories
OUTPUT_DIR=./output
TRAINING_DATA_DIR=./training_data
MODELS_DIR=./models

# Audio processing settings
SAMPLE_RATE=44100
VOCALS_ONLY=true

# RVC Model settings
RVC_MODEL_NAME=assamese_singer
TRAINING_EPOCHS=300
BATCH_SIZE=8

# Ethics and compliance
CONSENT_OBTAINED=false
RESEARCH_PURPOSE=true
ARTIST_NAME=
ARTIST_CONTACT=
